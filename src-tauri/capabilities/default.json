{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "fs:allow-home-write-recursive",
    "dialog:allow-open",
    "dialog:allow-ask",
    "dialog:allow-message",
    "core:window:default", "core:window:allow-start-dragging", "core:window:allow-close"
  ]
}
